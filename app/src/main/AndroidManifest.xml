<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.goksususuzlu_termproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectColorActivity"
            android:parentActivityName=".ModifyCarActivity">
        </activity>
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ModifyCarActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity"
            android:parentActivityName=".SelectModelActivity"/>
        <activity android:name=".BuildDetailsActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".DecorativeElementsActivity"
            android:parentActivityName=".ModifyCarActivity" />
        <activity
            android:name=".ModifySteeringWheelActivity"
            android:parentActivityName=".ModifyCarActivity" />
        <activity
            android:name=".ModifySeatFurnishingActivity"
            android:parentActivityName=".ModifySeatTypeActivity" />
        <activity
            android:name=".ModifySeatTypeActivity"
            android:parentActivityName=".ModifyCarActivity" />
        <activity
            android:name=".ModifyHeadlightsActivity"
            android:parentActivityName=".ModifyCarActivity" />
        <activity
            android:name=".ModifyRimsActivity"
            android:parentActivityName=".ModifyCarActivity" />
        <activity
            android:name=".SelectMotorActivity"
            android:parentActivityName=".SelectModelActivity" />
        <activity
            android:name=".SelectModelActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity"
            android:parentActivityName=".WelcomeActivity">

        </activity>
    </application>

</manifest>